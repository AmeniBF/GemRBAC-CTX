      <relative xsi:type="RBACmodel:DayOfWeek" overlay="//@c.1/@timeexpression.0/@relative.2" day="Sunday"/>
      <relative xsi:type="RBACmodel:RelativeTimeInterval" idRelativeTE="weekInterval" overlay="//@c.1/@timeexpression.0/@relative.2" idRelativeTimeInterval="">
        <start xsi:type="RBACmodel:DayOfWeek" day="Wednesday"/>
        <end xsi:type="RBACmodel:DayOfWeek" day="Friday"/>
      </relative>
      <relative xsi:type="RBACmodel:RelativeTimeInterval" idRelativeTE="hourInterval">
        <start xsi:type="RBACmodel:HourOfDay" hour="10"/>
        <end xsi:type="RBACmodel:HourOfDay" hour="12"/>
      </relative>
    </timeexpression>
  </c>
  <c xsi:type="RBACmodel:SpatialContext" idContext="posU1">
    <location xsi:type="RBACmodel:Point"/>
  </c>
  <c xsi:type="RBACmodel:SpatialContext" idContext="posU2">
    <location xsi:type="RBACmodel:Point"/>
  </c>
  <p roles="//@r.0 //@r.1" operations="//@op.0 //@op.1" idPermission="p1" object="//@Obj.0"/>
  <p roles="//@r.0" operations="//@op.1" idPermission="p2" object="//@Obj.1"/>
  <p roles="//@r.1" operations="//@op.2" idPermission="p3" object="//@Obj.2"/>
  <r idRole="r1" users="//@u.0 //@u.1" permissions="//@p.0 //@p.1" roleContextAssignment="//@c.1"/>
  <r idRole="r3" users="//@u.1" permissions="//@p.0 //@p.2"/>
  <u idUser="u1" roles="//@r.0" userLocation="//@c.2"/>
  <u idUser="u2" roles="//@r.1 //@r.0" userLocation="//@c.3"/>
</RBACmodel:Root>
