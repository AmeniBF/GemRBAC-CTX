      </relative>
      <relative xsi:type="RBACmodel:RelativeTimeInterval" idRelativeTE="hour2">
        <start xsi:type="RBACmodel:HourOfDay" hour="10"/>
        <end xsi:type="RBACmodel:HourOfDay" hour="15"/>
      </relative>
    </timeexpression>
    <timeexpression>
      <relative xsi:type="RBACmodel:MonthOfYear" month="12"/>
      <relative xsi:type="RBACmodel:MonthOfYear" month="2"/>
      <relative xsi:type="RBACmodel:RelativeTimeInterval" idRelativeTE="week">
        <start xsi:type="RBACmodel:DayOfWeek"/>
        <end xsi:type="RBACmodel:DayOfWeek" idRelativeTimePoint="" day="Friday"/>
      </relative>
      <relative xsi:type="RBACmodel:RelativeTimeInterval" idRelativeTE="hour">
        <start xsi:type="RBACmodel:HourOfDay" hour="8"/>
        <end xsi:type="RBACmodel:HourOfDay" hour="11"/>
      </relative>
    </timeexpression>
  </c>
  <c xsi:type="RBACmodel:SpatialContext" idContext="posU1">
